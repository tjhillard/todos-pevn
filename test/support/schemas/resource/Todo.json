{
  "$id": "https://example.com/todo.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Todo",
  "type": "object",
  "required": [
    "id", "user_id", "description", "completed", "created_at", "updated_at"
  ],
  "properties": {
    "id": {
      "type": "integer"
    },
    "user_id": {
      "type": "integer"
    },
    "description": {
      "type": "string"
    },
    "completed": {
      "type": "boolean"
    },
    "created_at": {
      "type": "string"
    },
    "updated_at": {
      "type": "string"
    }
  }
}